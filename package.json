{
  "name": "tgstorage",
  "version": "1.0.0",
  "description": "tgstorage",
  "author": "Alexander Shershnev",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sh-a-v/tgstorage.git"
  },
  "bugs": {
    "url": "https://github.com/sh-a-v/tgstorage/issues"
  },
  "homepage": "https://github.com/sh-a-v/tgstorage#readme",
  "scripts": {
    "dev": "npm run clean && BUILD_ENV=dev NODE_ENV=development webpack serve --progress",
    "build:stage": "npm run clean && BUILD_ENV=stage NODE_ENV=production webpack",
    "build:prod": "npm run clean && BUILD_ENV=prod NODE_ENV=production webpack",
    "clean": "rimraf build",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "analyze-bundle:modern": "npm run clean && BUILD_ENV=stage NODE_ENV=production BUNDLE_ANALYZER=modern webpack",
    "analyze-bundle:legacy": "npm run clean && BUILD_ENV=stage NODE_ENV=production BUNDLE_ANALYZER=legacy webpack",
    "check-es5": "npx are-you-es5 check ./",
    "generate-phone-metadata": "libphonenumber-metadata-generator metadata.custom.json --countries RU,BY,LV,UA,DE --extended",
    "generate-pwa-images": "npx pwa-asset-generator ./src/ui/icons/app.svg ./src/ui/images --index ./src/index.html --manifest ./src/index.webmanifest --background '#313131' --opaque false --type png --favicon"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.13.10",
    "@mtproto/core": "5.3.0",
    "classnames": "2.2.6",
    "core-js": "3.9.1",
    "idb-keyval": "5.0.4",
    "native-url": "0.3.4",
    "preact": "10.5.13",
    "regenerator-runtime": "0.13.7",
    "unistore": "3.5.2",
    "unistore-hooks": "0.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-export-namespace-from": "7.12.13",
    "@babel/plugin-transform-react-jsx": "7.13.12",
    "@babel/plugin-transform-runtime": "7.13.10",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-typescript": "7.13.0",
    "@types/classnames": "2.2.11",
    "@types/node": "14.14.35",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "autoprefixer": "10.2.5",
    "babel-loader": "8.2.2",
    "comlink-loader": "2.0.0",
    "copy-webpack-plugin": "8.1.0",
    "css-loader": "5.2.0",
    "dotenv": "8.2.0",
    "dotenv-webpack": "7.0.2",
    "eslint": "7.22.0",
    "eslint-plugin-react": "7.23.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.1",
    "libphonenumber-metadata-generator": "1.0.1",
    "mini-css-extract-plugin": "1.3.9",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "postcss": "8.2.8",
    "postcss-loader": "5.2.0",
    "postcss-safe-parser": "5.0.2",
    "preact-svg-loader": "0.2.1",
    "rimraf": "3.0.2",
    "robotstxt-webpack-plugin": "7.0.0",
    "style-loader": "2.0.0",
    "stylus": "0.54.8",
    "stylus-loader": "5.0.0",
    "terser-webpack-plugin": "5.1.1",
    "typescript": "4.2.3",
    "webpack": "5.28.0",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2",
    "webpack-module-nomodule-plugin": "1.0.1",
    "workbox-webpack-plugin": "6.1.2"
  }
